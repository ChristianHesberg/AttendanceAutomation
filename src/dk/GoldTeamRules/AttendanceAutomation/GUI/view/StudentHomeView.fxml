<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>

<BorderPane maxHeight="384.0" maxWidth="575.0" minHeight="384.0" minWidth="575.0" prefHeight="384.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.GoldTeamRules.AttendanceAutomation.GUI.controller.StudentHomeViewController">
   <top>
      <HBox prefHeight="41.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
         </padding>
         <children>
            <Button fx:id="searchBtn" mnemonicParsing="false" onAction="#handleSearch" text="Search">
               <HBox.margin>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </HBox.margin>
            </Button>
            <TextField fx:id="searchBar" prefHeight="26.0" prefWidth="139.0">
               <HBox.margin>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </HBox.margin>
            </TextField>
            <HBox alignment="TOP_RIGHT" prefHeight="34.0" prefWidth="194.0">
               <children>
                  <Button fx:id="confirmClassBtn" mnemonicParsing="false" onAction="#chooseClassClick" prefHeight="26.0" prefWidth="83.0" text="Confirm">
                     <HBox.margin>
                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <ChoiceBox fx:id="classChoiceBox" prefHeight="26.0" prefWidth="139.0">
               <HBox.margin>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </HBox.margin>
            </ChoiceBox>
         </children>
      </HBox>
   </top>
   <center>
      <TilePane fx:id="tilePane" hgap="50.0" prefHeight="200.0" prefWidth="200.0" vgap="50.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </TilePane>
   </center>
</BorderPane>
